{
  "name": "maplibre-gl-flight-simulator",
  "version": "1.0.0",
  "description": "A MapLibre GL JS plugin that provides aircraft motion control and simulation, enabling users to create interactive 3D flight experiences within MapLibre maps.",
  "main": "dist/maplibre-gl-flight-simulator.esm.js",
  "module": "dist/maplibre-gl-flight-simulator.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
  "clean": "rm -rf dist",
  "build:types": "tsc --emitDeclarationOnly",
  "build": "npm run clean && npm run build:types && webpack --config webpack.config.js",
  "build:watch": "webpack --watch --config webpack.config.js"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [ "maplibre", "maplibre-gl", "plugin", "flight", "simulator", "3D", "typescript" ],
  "author": {
    "name": "Samarth Otiya",
    "email": "samarthrobo03@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Samarth1696/maplibre-gl-flight-simulator"
  },
  "bugs": {
    "url": "https://github.com/Samarth1696/maplibre-gl-flight-simulator/issues"
  },
  "devDependencies": {
    "@types/maplibre-gl": "^1.13.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "maplibre-gl": "^5.0.0-pre.4"
  },
  "dependencies": {
    "maplibre-gl": "^5.0.0-pre.4"
  },
  "exports": {
  ".": {
    "import": "./dist/maplibre-gl-flight-simulator.esm.js",
    "require": "./dist/maplibre-gl-flight-simulator.umd.js",
    "types": "./dist/types/index.d.ts"
  }
 }
}
